{
  "name": "acai-agents-website",
  "version": "2.0.0",
  "description": "AI-Powered Software Development Agency - Official Website",
  "type": "module",
  "private": true,
  "author": {
    "name": "Acai Agents",
    "email": "hello@acai-agents.dev",
    "url": "https://acai-agents.dev"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/acai-agents/website.git"
  },
  "homepage": "https://acai-agents.dev",
  "bugs": {
    "url": "https://github.com/acai-agents/website/issues"
  },
  "keywords": [
    "acai-agents",
    "ai",
    "software-development",
    "agency",
    "website"
  ],
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "dev": "node scripts/dev-server.js",
    "dev:live": "npx live-server --port=3000 --host=localhost",
    "start": "node scripts/dev-server.js",
    "lint": "npx eslint js/**/*.js",
    "lint:fix": "npx eslint js/**/*.js --fix",
    "format": "npx prettier --write \"**/*.{js,css,html,md,json}\"",
    "format:check": "npx prettier --check \"**/*.{js,css,html,md,json}\"",
    "build:css": "npx postcss css/*.css --dir dist/css",
    "build:js": "npx terser js/utils.js js/app.js js/gsap-animations.js -o dist/js/bundle.min.js --compress --mangle",
    "build": "node scripts/build.js",
    "clean": "node --input-type=module -e \"import fs from 'node:fs'; fs.rmSync('dist', { recursive: true, force: true });\"",
    "validate": "npm run lint && npm run format:check"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "autoprefixer": "^10.4.23",
    "cssnano": "^7.1.2",
    "eslint": "^9.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-security": "^3.0.0",
    "globals": "^15.0.0",
    "live-server": "^1.2.2",
    "postcss": "^8.4.0",
    "postcss-cli": "^11.0.0",
    "prettier": "^3.4.0",
    "terser": "^5.36.0"
  },
  "browserslist": [
    ">0.3%",
    "last 2 versions",
    "not dead",
    "not op_mini all"
  ]
}
